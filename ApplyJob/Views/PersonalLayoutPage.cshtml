        <div class="container">
            <div class="head">

                <nav class=”navbar navbar-primary" role="navigation">
                    <div class="navbar-header">
                        <a href="#" class="navbar-brand">  <img src="~/image/logo.jpg" width="250" height="80" /></a>
                    </div>
                    <ul class="nav  nav-justified">
