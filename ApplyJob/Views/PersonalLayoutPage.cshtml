    <style>
        .tbar {
            width: 100%;
            height: 40px;
            background: #3291f8;
        }

            .tbar ul li {
                padding: 8px 15px;
                float: right;
                list-style: none;
            }

                .tbar ul li a {
                    color: #fff;
                }

        .head {
            width: 100%;
            height: 80px;
            font-size: medium;
        }

        .navbar-brand {
            padding: 0px;
            float: left;
        }

        .nav {
            margin: 10px auto;
        }

            .nav li a {
                top: 20px;
            }
    </style>

</head>
<body>
    <div>
